# Environment Variables Example
# Copy this file to .env.local and update the values

# Backend Middle URL for lift up commands
NEXT_PUBLIC_BACKEND_MIDDLE_URL=https://example.com/api/lift-up

# RSPI Server URL for control commands
NEXT_PUBLIC_RSPI_SERVER_YOKYOR=https://example.com/api

# API URL for your main backend
NEXT_PUBLIC_API_URL=http://localhost:8000

# DeepSeek API Key (if using)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Gemini API Key (if using)
GEMINI_API_KEY=your_gemini_api_key_here
